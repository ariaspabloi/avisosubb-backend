{
  "useTabs": false,
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "jsxBracketSameLine": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "semi": false,
  "endOfLine": "auto",
  "spaceBeforeFunctionParent": { "anonymous": "always", "named": "always" }
}
